package com.models;

public class Cozinheiro {
	private String nome;
    private int idMesa;

    public Cozinheiro(String nome, int idMesa) {
        this.nome = nome;
        this.idMesa = idMesa;
    }

    public String getNome() {
        return nome;
    }

    public int getIdMesa() {
        return idMesa;
    }
}
