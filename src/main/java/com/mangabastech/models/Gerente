package com.models;

import java.util.ArrayList;
import java.util.List;

public class Gerente {
    private String nome;
    private List<String> pratos = new ArrayList<>();

    public Gerente(String nome) {
        this.nome = nome;
    }

    public void cadastrarPrato(String prato) {
        pratos.add(prato);
    }

    public List<String> getPratos() {
        return pratos;
    }
}
